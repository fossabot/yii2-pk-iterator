{
    "name": "serhioli/yii2-pk-iterator",
    "description": "BatchQueryResult based on iterating via ids range in query result set",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Lysogor",
            "email": "me@serhio.li"
        }
    ],
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "yiisoft/yii2-composer": true
        },
        "platform": {
            "ext-mcrypt": "1.0.4"
        }
    },
    "require": {
        "php": ">= 7.3",
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.17"
    },
    "autoload": {
        "psr-4": {
            "serhioli\\Yii2\\Db\\PkIterator\\": "src/",
            "serhioli\\Yii2\\Db\\PkIterator\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
